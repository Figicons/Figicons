{
  "name": "figcons",
  "version": "1.0.1",
  "main": "index.js",
  "author": "Stefan Mansson <st.mansson@icloud.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel ./src/index.html",
    "deploy": "rimraf ./build && parcel build ./src/index.html -d ./build --no-source-maps && cp _redirects ./build/_redirects && netlify deploy ./dist",
    "build": "sh ./build.sh",
    "bundle": "webpack",
    "clean": "svgo --quiet --config=./svgo.yml -f ./svgs -o ./svgs",
    "fetch": "node --harmony fetch.js",
    "parse": "node --harmony parse.js"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "^1.2.1",
    "camelcase": "^5.0.0",
    "cheerio": "^1.0.0-rc.2",
    "parse5": "^5.1.0",
    "react": ">=15",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/react": "^16.4.18",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "svgo": "^1.1.1",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.4",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  }
}
